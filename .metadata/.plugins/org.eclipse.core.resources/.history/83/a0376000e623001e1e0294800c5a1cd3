package com.ust.cart.VO;

import com.ust.cart.Model.Cart;

public class RestTemplateVO {
	private Order order;
	private Cart cart;
	
	public RestTemplateVO() {
		// TODO Auto-generated constructor stub
	}

	public RestTemplateVO(Order order, Cart cart) {
		super();
		this.order = order;
		this.cart = cart;
	}

	public Order getOrder() {
		return order;
	}

	public void setOrder(Order order) {
		this.order = order;
	}

	public Cart getCart() {
		return cart;
	}

	public void setCart(Cart cart) {
		this.cart = cart;
	}
	

}
